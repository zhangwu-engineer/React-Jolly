{
  "name": "jolly-ui",
  "version": "2.0.0",
  "private": true,
  "description": "A highly scalable, offline-first foundation with the best DX and a focus on performance and best practices",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": "6.4.1",
    "node": "8.12.0"
  },
  "author": "Max Stoiber",
  "license": "MIT",
  "scripts": {
    "clean:hard": "rm -Rf ./node_modules/* && rm -Rf ./build/*",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "preinstall": "node ./internals/scripts/checkversion.js",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "webpack --config internals/webpack/webpack.prod.babel.js --color --progress",
    "build:clean": "npm run test:clean",
    "build:dev": "webpack -d --config internals/webpack/webpack.dev.babel.js --color --progress",
    "build:dll": "BUILD_LIB=true node ./internals/scripts/dependencies.js",
    "build:prod": "NODE_ENV=production npm run build",
    "start": "node server",
    "start:production": "npm run test && npm run build:prod && NODE_ENV=production node server",
    "foreverstart": "NODE_ENV=production forever start --minUptime 10000 --spinSleepTime 10000 --uid jolly-ui --append server/index.js",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:fix": "npm run lint:eslint -- . --fix",
    "lint:staged": "lint-staged",
    "pretest:coverage": "npm run test:clean",
    "test:clean": "rimraf ./coverage",
    "test": "NODE_ENV=test jest",
    "test:update": "NODE_ENV=test jest --updateSnapshot",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --watch",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "heroku-postbuild": "npm run build:prod"
  },
  "lint-staged": {
    "*.js": "lint:eslint",
    "*.{js,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": [
    "lint:staged"
  ],
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "express",
      "ip",
      "minimist"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "@date-io/date-fns": "0.0.2",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "babel-polyfill": "6.20.0",
    "bluebird": "^3.5.0",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.7.3",
    "date-fns": "^2.0.0-alpha.25",
    "draft-js": "^0.10.4",
    "draftjs-to-html": "^0.8.2",
    "express": "4.16.3",
    "express-sslify": "^1.2.0",
    "fontfaceobserver": "2.0.7",
    "history": "4.6.3",
    "html-to-draftjs": "^1.1.2",
    "immutability-helper": "^2.8.1",
    "immutable": "3.8.2",
    "ip": "1.1.4",
    "lodash-es": "^4.17.11",
    "marked": "^0.3.6",
    "material-ui-pickers": "^2.0.4",
    "mime-types": "^2.1.16",
    "minimist": "1.2.0",
    "pace": "0.0.4",
    "pace-js": "^1.0.2",
    "prerender-node": "^3.2.0",
    "progressbar.js": "^1.0.1",
    "qs": "^6.4.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-dropzone": "^7.0.1",
    "react-foundation-components": "0.14.0",
    "react-helmet": "^5.2.0",
    "react-image-lightbox": "^5.1.0",
    "react-loadable": "^5.3.1",
    "react-masonry-component": "^6.2.1",
    "react-modal": "3.4.2",
    "react-phone-number-input": "^2.2.16",
    "react-places-autocomplete": "^7.2.0",
    "react-redux": "^5.0.7",
    "react-redux-toastr": "^7.0.0",
    "react-router": "4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-hash-link": "^1.2.0",
    "react-router-redux": "5.0.0-alpha.6",
    "react-router-scroll-memory": "^1.0.1",
    "react-share": "^1.19.1",
    "react-social-login": "^3.4.2",
    "react-waypoint": "^8.1.0",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-saga": "0.16.0",
    "request-promise": "^4.2.1",
    "reselect": "2.5.4",
    "shortid": "^2.2.8",
    "sitemap": "^1.13.0",
    "store": "^2.0.4",
    "tls": "0.0.1",
    "tunnel-agent": "^0.6.0",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "ajv": "^6.5.2",
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "6.26.0",
    "babel-core": "6.24.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "cheerio": "0.22.0",
    "circular-dependency-plugin": "^5.0.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "empty-module": "0.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-redux-saga": "0.1.5",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "flow-bin": "0.72.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.5.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.2.0",
    "jest-cli": "^23.2.0",
    "lint-staged": "^3.2.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-plop": "0.5.4",
    "node-sass": "^4.9.2",
    "null-loader": "0.1.1",
    "plop": "1.7.3",
    "postcss-loader": "^2.1.3",
    "pre-commit": "1.1.3",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react-test-renderer": "16.3.2",
    "redux-logger": "^2.10.2",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.7",
    "shelljs": "0.7.5",
    "sinon": "2.0.0-pre",
    "style-loader": "^0.20.3",
    "svg-sprite-loader": "^4.1.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.0",
    "webpack-pwa-manifest": "^3.6.2"
  }
}
